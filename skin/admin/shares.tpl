<div id="shares-{id}" shares-id="{id}" class="shares editblock clear">

    <div class="desc">
            <div class="title {titleClass} field" spellcheck="false" default="Без названия" contenteditable="true">{title}</div>
            <div class="text  {textClass} field" spellcheck="false" default="Без цены" contenteditable="true">{text}</div>
    </div>

    <div class="actions">
		
		<div class="action delete" onclick="deleteBlock('shares', {id});"><i class="fa fa-trash-o" aria-hidden="true"></i></div>
	</div>

</div>